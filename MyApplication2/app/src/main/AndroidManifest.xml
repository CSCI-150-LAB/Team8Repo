<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mycar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.mycar.Reservation"
            android:parentActivityName="com.example.mycar.Dashboard">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mycar.Dashboard" />
        </activity> <!-- ============================================================================= -->
        <activity
            android:name="com.example.mycar.Cart"
            android:parentActivityName="com.example.mycar.Dashboard">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mycar.Dashboard" />
        </activity> <!-- ============================================================================= -->
        <activity android:name="com.example.mycar.Dashboard" /> <!-- ============================================================================= -->
        <activity android:name="com.example.mycar.Register" /> <!-- ============================================================================= -->
        <activity android:name="com.example.mycar.DealerSignUp" /> <!-- ============================================================================= -->
        <activity android:name="com.example.mycar.AdminSignUp" /> <!-- ============================================================================= -->
        <activity android:name="com.example.mycar.Login" /> <!-- ============================================================================= -->
        <activity android:name="com.example.mycar.DealerLogin" /> <!-- ============================================================================= -->
        <activity android:name="com.example.mycar.AdminLogin" /> <!-- ============================================================================= -->
        <activity android:name="com.example.mycar.ForgotPassword" /> <!-- ============================================================================= -->
        <activity android:name="com.example.mycar.Settings"
            android:parentActivityName="com.example.mycar.Dashboard">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mycar.Dashboard" />
        </activity>
        <activity android:name="com.example.mycar.AbouUs"
            android:parentActivityName="com.example.mycar.Dashboard">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mycar.Dashboard" />
        </activity>
        <activity android:name=".AdminModUser"
            android:parentActivityName="com.example.mycar.AdminBoard">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mycar.AdminBoard" />
        </activity>
        <activity android:name="com.example.mycar.Home">
            <!-- ___________ STARTING ACTIVITY CODE ___________ -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <!-- ______________________________________________ -->
        </activity> <!-- ============================================================================= -->
        <activity android:name="com.example.mycar.DealerDashBoard" /> <!-- ============================================================================= -->
        <activity android:name="com.example.mycar.AdminBoard" /> <!-- ============================================================================= -->
        <activity android:name="com.example.mycar.Userprofile" /> <!-- ============================================================================= -->
        <activity android:name="com.example.mycar.EditProfile" /> <!-- ============================================================================= -->
        <activity android:name="com.example.mycar.ResetPassword" /> <!-- ============================================================================= -->
        <activity
            android:name="com.example.mycar.Search"
            android:parentActivityName="com.example.mycar.Dashboard">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mycar.Dashboard" />
        </activity> <!-- ============================================================================= -->
        <activity
            android:name="com.example.mycar.Select"
            android:parentActivityName="com.example.mycar.Search">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mycar.Search" />
        </activity> <!-- ============================================================================= -->
        <activity
            android:name="com.example.mycar.Calendar"
            android:parentActivityName="com.example.mycar.Dashboard">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mycar.Dashboard" />
        </activity>
    </application>

</manifest>